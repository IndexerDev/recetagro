<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Bienvenido a RecetAgro</title>        
    @Styles.Render("~/Content/css")    
    @Scripts.Render("~/bundles/modernizr")        
</head>
<body>
   
    <div class="container loginPaddingTop">
        @RenderBody()
        <hr />        
    </div>
    

    <footer>
        <div class="container">
            <p>&copy; @DateTime.Now.Year - RecetAgro powered by WS</p>    
        </div>        
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)    

</body>
</html>
